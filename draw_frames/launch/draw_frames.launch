<launch>
  <param name="/use_sim_time" value="True" />

  <node name="republish" type="republish" pkg="image_transport" output="screen" args="compressed in:=/HX09/camera/color/image_raw raw out:=/HX09/camera/color/image_raw" />

  <node name="draw_frames" pkg="draw_frames" type="draw_frames" args="/HX04" output="screen">  <!-- /base_laser_link /r_gripper_tool_frame -->
    <remap from="image" to="/HX09/camera/color/image_raw" />
  </node>

  <node name="input_viewer" pkg="image_view" type="image_view">
    <remap from="image" to="/HX09/camera/color/image_raw" />
    <param name="autosize" value="True" />
  </node>

  <node name="output_viewer" pkg="image_view" type="image_view">
    <remap from="image" to="image_out" />
    <param name="autosize" value="True" />
  </node>
</launch>